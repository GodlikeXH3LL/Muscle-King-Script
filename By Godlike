local Credits = Window:CreateTab("Credits", "file-badge-2")
local LinkedDiscords = Window:CreateTab("Linked Discords", "bot")

local Input = Main:CreateInput({
   Name = "Size Changer",
   CurrentValue = "2",
   PlaceholderText = "Choose Ur Size",
   RemoveTextAfterFocusLost = false,
   Flag = "Size",
   Callback = function(Text)
       local selectedSize = Text
       if _G.AutoSize then
           game:GetService("ReplicatedStorage").rEvents.changeSpeedSizeRemote:InvokeServer("changeSize", tonumber(selectedSize))
       end
   end
})
