Rayfield:Notify({
   Title = "Hello",
   Content = "Welcome isayt5729",
   Duration = 3.5,
   Image = "dumbell",
})

local Main = Window:CreateTab("Main", "moon")
local AutoWeight = Window:CreateTab("Auto Weight", "earth-lock")
local AutoRebirth = Window:CreateTab("Auto Rebirth", "haze")
local AutoKill = Window:CreateTab("Auto Kill", "utensils")
local AutoRock = Window:CreateTab("Auto Rock", "mountain")
local Teleports = Window:CreateTab("Teleports", "door-closed")
local OPTOOLS = Window:CreateTab("OP TOOLS", "power")
